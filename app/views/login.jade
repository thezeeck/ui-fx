doctype html
html 
  include ../jade/head.jade
  body
    div.appContent
      section#logIn
        figure
          img#logoLogin(src="img/anova_color.png" alt="")
        div.titleLogin
          i.fa.fa-sign-in  Enter data to login
          form
            ul
              li
                input(type="text" name="username" value="" placeholder="Username")
              li
                input(type="text" name="password" value="" placeholder="Password")
              li
                a.button.primary(href="index.html") Send
          div(class="textLogin")
            a(href="#") Forgot your password?
